{
  "name": "linkedfarm/wp-lf-delivery-option",
  "type": "wordpress-plugin",
  "description": "Linked Farm Delivery Option extension plugin for lf-hub-wc",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Kalin Maldzhanski",
      "email": "k@djodjo.org",
      "homepage": "http://djodjo.org",
      "role": "Developer"
    }
  ],
  "keywords": ["wordpress", "plugin", "LinkedFarm", "OpenFoodData", "FoodActor"],
  "homepage": "https://linkedfarm.github.io/wp-lf-delivery-option",
  "support": {
    "issues": "https://github.com/LinkedFarm/wp-lf-delivery-option/issues",
